<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema elementFormDefault="qualified"
	targetNamespace="urn:esbbank.com/gbo/xml/schemas/v1_0/" version="1.0"
	xmlns:esbbankgbo="urn:esbbank.com/gbo/xml/schemas/v1_0/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:complexType name="EAI_HEADER">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="referenceNum"
				type="esbbankgbo:ReferenceNum" />
			<xsd:element minOccurs="1" name="clientChannel"
				type="esbbankgbo:ClientChannel" />
			<xsd:element minOccurs="1" name="requestTime" type="esbbankgbo:TimeStamp" />
			<xsd:element minOccurs="0" name="retryFlag" type="esbbankgbo:Flag" />
			<xsd:element minOccurs="0" name="sucessfulReversalFlag"
				type="esbbankgbo:Flag" />
			<xsd:element minOccurs="0" name="SnapIns" type="esbbankgbo:SnapIns" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="EAI_STATUS">
		<xsd:annotation>
			<xsd:documentation>
				in case of Successfully message processing :
				Return Status Code
				Provider Should be 000000 and the desc is Success
				in case of Failure message processing :
				Return Status Code Provider
				Should be the actual back end system error code and its description

			</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="returnStatus"
				type="esbbankgbo:ReturnStatus" />
			<xsd:element maxOccurs="1" minOccurs="0"
				name="returnStatusProvider" type="esbbankgbo:ReturnStatusProvider" />
			<!-- <xsd:element maxOccurs="1" minOccurs="1" name="returnStatusProvider" 
				type="esbbankgbo:ReturnStatusProvider" /> -->
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="ReturnStatus">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="returnCode"
				type="esbbankgbo:ReturnCode" />
			<xsd:element maxOccurs="1" minOccurs="0" name="returnCodeDesc"
				type="esbbankgbo:ReturnCodeDesc" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ReturnStatusProvider">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="returnCodeProvider"
				type="esbbankgbo:ReturnCodeProvider" />
			<xsd:element maxOccurs="1" minOccurs="0"
				name="returnCodeDescProvider" type="esbbankgbo:ReturnCodeDesc" />
		</xsd:sequence>
	</xsd:complexType>




	<xsd:complexType name="Period">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="fromDate"
				type="esbbankgbo:Date" />
			<xsd:element maxOccurs="1" minOccurs="1" name="toDate"
				type="esbbankgbo:Date" />
		</xsd:sequence>
	</xsd:complexType>



	<xsd:complexType name="SecurityInfo">
		<xsd:sequence>
			<xsd:element name="authentication" type="esbbankgbo:Authentication" />
			<xsd:element minOccurs="0" name="authorization"
				type="esbbankgbo:Authorization" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Authentication">
		<xsd:sequence>
			<xsd:element name="UserId" type="esbbankgbo:String" />
			<xsd:element name="Password" type="esbbankgbo:String" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Authorization">
		<xsd:sequence>
			<xsd:element name="UserId" type="esbbankgbo:String" />
		</xsd:sequence>
	</xsd:complexType>



	<xsd:complexType name="NameValue">
		<xsd:attribute name="Name" type="esbbankgbo:String" />
		<xsd:attribute name="Value" type="esbbankgbo:String" />
	</xsd:complexType>

	<xsd:complexType name="SnapIns" mixed="true">
		<xsd:sequence>

			<xsd:element name="nameValue" type="esbbankgbo:NameValue"
				maxOccurs="unbounded" minOccurs="1"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="Flag">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:enumeration value="Y" />
			<xsd:enumeration value="N" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ServiceName">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="30" />
			<xsd:pattern value="[a-zA-Z0-9.]*" />
		</xsd:restriction>
	</xsd:simpleType>



	<xsd:simpleType name="ClientChannel">
		<xsd:annotation>
			<xsd:documentation>
				MOB = Mobile
				IVR = IVR
				CIB = Corporate Banking
				RIB =
				Retail Banking
				ATM =
				ATM Switch
				TF = Trade Finance
				TR = Tressury
				ECC =
				Electronic Cheque Clearing
				BKR = Internal Call or
				for Testing
				SAL =
				Salary Processing
				IB = Inter Blocks
				CBS = Core Banking system
				CRM =Customer relation managment
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="esbbankgbo:String">
<!-- 
			<xsd:enumeration value="NA" />
			<xsd:enumeration value="MOB" />
			<xsd:enumeration value="IVR" />
			<xsd:enumeration value="CIB" />
			<xsd:enumeration value="RIB" />
			<xsd:enumeration value="ATM" />
			<xsd:enumeration value="SAL" />
			<xsd:enumeration value="TF" />
			<xsd:enumeration value="TR" />
			<xsd:enumeration value="ECC" />
			<xsd:enumeration value="BKR" />
			<xsd:enumeration value="IB" />
			<xsd:enumeration value="CBS" />
			<xsd:enumeration value="CMS" />
 -->
 		</xsd:restriction>
		
	</xsd:simpleType>

	<xsd:simpleType name="MsgChannel">
		<xsd:annotation>
			<xsd:documentation>
				MQ = MQ
				HTTP = HTTP
				WS = Webservice
				TCIP = TCP IP
				FILE = File Based
				BKR =
				Internal Call or for Testing
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="esbbankgbo:String">
			<xsd:enumeration value="MQ" />
			<xsd:enumeration value="HTTP" />
			<xsd:enumeration value="WS" />
			<xsd:enumeration value="TCIP" />
			<xsd:enumeration value="FILE" />
			<xsd:enumeration value="BKR" />
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:simpleType name="ReturnCode">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1"></xsd:minLength>
			<xsd:maxLength value="6"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ReferenceNum">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="24" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ReferenceNumConsumer">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="120" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ReferenceNumProvider">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="120" />
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:simpleType name="RetryMsg">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:length value="1"></xsd:length>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ReturnCodeDesc">
		<xsd:restriction base="esbbankgbo:String">
<!-- 
			<xsd:minLength value="0" />
			<xsd:maxLength value="128" />
 -->				
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ReturnCodeProvider">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="24" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="AcctType">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="10" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="AcctClassCode">
		<xsd:restriction base="esbbankgbo:String">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="AcctStatus">
		<xsd:restriction base="esbbankgbo:String">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="IBANNumber">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:maxLength value="35"></xsd:maxLength>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="AcctNumber">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="13" />
			<xsd:maxLength value="13" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="OutsideAcctNumber">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="34" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="CustomerNumber">
		<xsd:restriction base="esbbankgbo:String">


			<xsd:length value="6"></xsd:length>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="CustomerType">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="CustomerSegment">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Gender">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:enumeration value="M" />
			<xsd:enumeration value="F" />
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:simpleType name="CurrencyCode">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:length value="3" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Amount">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="16" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Name">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="0" />
			<xsd:maxLength value="256" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ShortName">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="0" />
			<xsd:maxLength value="35" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="PersonName">
		<xsd:sequence>
			<xsd:element name="nickName" type="esbbankgbo:ShortName"
				maxOccurs="1" minOccurs="0"></xsd:element>
			<xsd:element minOccurs="0" name="fullName" type="esbbankgbo:Name" />
			<xsd:element minOccurs="0" name="firstName" type="esbbankgbo:ShortName" />
			<xsd:element minOccurs="0" name="LastName" type="esbbankgbo:ShortName" />
			<xsd:element minOccurs="0" name="SecondName" type="esbbankgbo:ShortName" />
			<xsd:element minOccurs="0" name="ThirdName" type="esbbankgbo:ShortName" />
			<xsd:element minOccurs="0" name="FourthName" type="esbbankgbo:ShortName" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="IdType">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="50" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="IdNumber">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:maxLength value="24" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="PhoneNumber">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:maxLength value="24" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="Id">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="idType" type="esbbankgbo:IdType" />
			<xsd:element minOccurs="0" name="idNumber" type="esbbankgbo:IdNumber" />
			<xsd:element minOccurs="0" name="expirtyDate" type="esbbankgbo:Date" />
			<xsd:element minOccurs="0" name="issueDate" type="esbbankgbo:Date" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="AddressType">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="10" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="QIDType1">
		<xsd:restriction base="esbbankgbo:String">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="QIDType">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="11" />
			<xsd:maxLength value="11" />
			<xsd:pattern value="\d+"></xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="ContactDetails">
		<xsd:all>
			<xsd:element minOccurs="0" name="addressType" type="esbbankgbo:AddressType" />
			<xsd:element minOccurs="0" name="phone" type="esbbankgbo:PhoneNumber" />
			<xsd:element name="phoneOffice" type="esbbankgbo:PhoneNumber"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
			<xsd:element name="phoneHome" type="esbbankgbo:PhoneNumber"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
			<xsd:element minOccurs="0" name="Mobile" type="esbbankgbo:PhoneNumber" />
			<xsd:element minOccurs="0" name="fax" type="esbbankgbo:PhoneNumber" />
			<xsd:element name="email" type="esbbankgbo:EmailAddress"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
			<xsd:element minOccurs="0" name="fullAddress" type="esbbankgbo:Name" />
			<xsd:element minOccurs="0" name="streetNum" type="esbbankgbo:Name" />
			<xsd:element minOccurs="0" name="streetName" type="esbbankgbo:Name" />
			<xsd:element minOccurs="0" name="boxNumber" type="esbbankgbo:Name" />
			<xsd:element minOccurs="0" name="city" type="esbbankgbo:Name" />
			<xsd:element name="state" type="esbbankgbo:Name"
				maxOccurs="1" minOccurs="0">
			</xsd:element>
			<xsd:element minOccurs="0" name="Country" type="esbbankgbo:Name" />
		</xsd:all>
	</xsd:complexType>

	<xsd:simpleType name="BranchCode">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="BankCode">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="CheckDetails">
		<xsd:sequence>
			<xsd:element name="serial" type="esbbankgbo:CheckNum"
				maxOccurs="1" minOccurs="1" />
			<xsd:element name="sequence" type="esbbankgbo:CheckNum"
				maxOccurs="1" minOccurs="1" />
			<xsd:element name="amount" type="esbbankgbo:Amount"
				maxOccurs="1" minOccurs="1" />
			<xsd:element name="date" type="esbbankgbo:Date"
				maxOccurs="1" minOccurs="1" />
			<xsd:element name="currencyCode" type="esbbankgbo:CurrencyCode"
				maxOccurs="1" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="BankInfo">
		<xsd:sequence>
			<xsd:element name="BankCode" type="esbbankgbo:BankCode"
				maxOccurs="1" minOccurs="0" />
			<xsd:element name="BankBranchCode" type="esbbankgbo:BranchCode"
				maxOccurs="1" minOccurs="0" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="CheckNum">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="96" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="CheckStatus">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:enumeration value="Accepted" />
			<xsd:enumeration value="Rejected" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NickName">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="30" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:complexType name="BeneficiaryID">
		<xsd:sequence>
			<xsd:element name="customerNumber" type="esbbankgbo:CustomerNumber"
				maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element minOccurs="1" name="nickName" type="esbbankgbo:NickName" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="BenificiaryUtilInfo">
		<xsd:sequence>
			<xsd:element name="debitAccount" type="esbbankgbo:AcctNumber"
				maxOccurs="1" minOccurs="0"></xsd:element>
			<xsd:element name="customerNumber" type="esbbankgbo:CustomerNumber"
				maxOccurs="1" minOccurs="1">
			</xsd:element>
			<xsd:element minOccurs="1" name="nickName" type="esbbankgbo:NickName" />

		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="PaymentDescription">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="0" />
			<xsd:maxLength value="35" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ChargeCodeType">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="0" />
			<xsd:maxLength value="2" />
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:complexType name="PostingAccount">
		<xsd:sequence>
			<xsd:element name="acctNumber" type="esbbankgbo:AcctNumber"  maxOccurs="1" minOccurs="1" />
			<xsd:element name="currencyCode" type="esbbankgbo:CurrencyCode" maxOccurs="1" minOccurs="1" />
			<xsd:element name="amount" type="esbbankgbo:Amount"		maxOccurs="1" minOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TransferCharges">
		<xsd:sequence>
			<xsd:element name="debitAcctNumber" type="esbbankgbo:AcctNumber"  maxOccurs="1" minOccurs="1" />
			<xsd:element name="debitCurrencyCode" type="esbbankgbo:CurrencyCode" maxOccurs="1" minOccurs="1" />
			<xsd:element name="chargeAmount" type="esbbankgbo:Amount" maxOccurs="1" minOccurs="1"/>
		<xsd:choice>
			<xsd:element name="chargeCode" type="esbbankgbo:ChargeCodeType" maxOccurs="1" minOccurs="1" />
			<xsd:element name="GLAcctNumber" type="esbbankgbo:AcctNumber"  maxOccurs="1" minOccurs="1" />
		</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>

	<!-- ChargeCodeDetail is for ECC services only  -->
	<xsd:complexType name="ChargeCodeDetail">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="chargeCode" type="esbbankgbo:Int" />
			<xsd:element minOccurs="1" name="chargeAmount" type="esbbankgbo:Amount" />
			<xsd:element minOccurs="0" name="chargeCurrencyCode"
				type="esbbankgbo:CurrencyCode" />
			<xsd:element minOccurs="0" name="chargeValueDate" type="esbbankgbo:Date" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="Date">
		<xsd:restriction base="xsd:date">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="String">
		<xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Int">
		<xsd:restriction base="xsd:int">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Decimal">
		<xsd:restriction base="xsd:decimal">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="TimeStamp">
		<xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Time">
		<xsd:restriction base="xsd:time">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Boolean">
		<xsd:restriction base="xsd:boolean">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Byte">
		<xsd:restriction base="xsd:byte">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Float">
		<xsd:restriction base="xsd:float">
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:simpleType name="CustomerStatus">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="25" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="DateofBirth">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:minLength value="1" />
			<xsd:maxLength value="10" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Language">
		<xsd:restriction base="esbbankgbo:String">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="Description">
		<xsd:restriction base="esbbankgbo:String">
			<xsd:maxLength value="256" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ErrorCode">
		<xsd:restriction base="esbbankgbo:String"></xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="StatusCode">
		<xsd:restriction base="esbbankgbo:String"></xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="BrokerName">
		<xsd:restriction base="esbbankgbo:String"></xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="FlowName">
		<xsd:restriction base="esbbankgbo:String"></xsd:restriction>
	</xsd:simpleType>


	<xsd:simpleType name="StatusMessage">
		<xsd:restriction base="esbbankgbo:String"></xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="IsVIP">
		<xsd:restriction base="esbbankgbo:Flag"></xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="CheckSeqNum">
		<xsd:restriction base="esbbankgbo:String"></xsd:restriction>
	</xsd:simpleType>


	<xsd:simpleType name="CardNumber">
		<xsd:restriction base="esbbankgbo:String">


			<xsd:minLength value="14"></xsd:minLength>
